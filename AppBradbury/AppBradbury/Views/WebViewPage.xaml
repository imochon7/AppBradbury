<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppBradbury.Views.WebViewPage"
             xmlns:local="clr-namespace:AppBradbury;assembly=AppBradbury"
             Title="PDF Viewer">
    <ContentPage.Content>
        <local:CustomWebView x:Name="customWebView" Uri="{Binding PDFSource}" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" />
    </ContentPage.Content>
</ContentPage>